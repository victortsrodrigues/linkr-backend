generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Posts {
  id    Int     @id @default(autoincrement())
  userId Int
  url String
  description String
  likes String[] @default([])
  date DateTime @default(now())
  dataTitle String
  dataDescription String
  dataImage String

  @@map("posts")
}